@{
    ViewBag.Title = "BingoCard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@* Body Card *@
<h1></h1>
<div class="row">
    <div class="col-md-12">
        @foreach (var item in Model)
        {
            <div class="btn-group btn-group-justified">
                @foreach (int number in item)
                {

                    var square = string.Empty;
                    square = (number == 0) ? "" : number.ToString();
                    <a class="btn btn-default btn-lg" style="display: width: 100px; height: 100px;" href="#" role="button">@square</a>
                }
            </div>
        }
    </div>
</div>
    

